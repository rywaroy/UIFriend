{
    "manifest_version": 2,
  
    "name": "UIFriend",
    "description": "",
    "version": "1.0",
  
    "permissions": [],
    "icons": {
      "16": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
