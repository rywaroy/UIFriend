{
    "manifest_version": 2,
    "author": "zhangzhihao<464485682@qq.com>",
    "name": "UIFriend",
    "description": "",
    "version": "1.0",
    "permissions": [],
    "icons": {
      "16": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/index.js"],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [
        "src/*"
    ]
  }